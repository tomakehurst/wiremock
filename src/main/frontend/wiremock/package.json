{
  "name": "wiremock",
  "version": "1.0.2",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --base-href=/",
    "buildOld": "ng build --prod --aot --base-href=/__admin/webapp/",
    "start-i18n:de": "ng serve --base-href=/ --aot --i18n-file=src/i18n/messages.de.xlf --i18n-format=xlf --locale=de",
    "build-i18n:de": "ng build --prod --base-href=/__admin/webapp/de --output-path=../../resources/assets/webapp/de --aot --i18n-file=src/i18n/messages.de.xlf --i18n-format=xlf locale=de",
    "build-i18n:en": "ng build --prod --base-href=/__admin/webapp/en --output-path=../../resources/assets/webapp/en --aot --i18n-file=src/i18n/messages.en.xlf --i18n-format=xlf locale=en",
    "build-i18n" : "yarn run build-i18n:de && yarn run build-i18n:en",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "i18n": "ng xi18n"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.3.2",
    "@angular/cdk": "^2.0.0-beta.8",
    "@angular/common": "^4.3.2",
    "@angular/compiler": "^4.3.2",
    "@angular/core": "^4.3.2",
    "@angular/forms": "^4.3.2",
    "@angular/http": "^4.3.2",
    "@angular/material": "^2.0.0-beta.8",
    "@angular/platform-browser": "^4.3.2",
    "@angular/platform-browser-dynamic": "^4.3.2",
    "@angular/router": "^4.3.2",
    "core-js": "^2.4.1",
    "highlightjs": "^9.10.0",
    "rxjs": "^5.4.2",
    "vkbeautify": "^0.99.2",
    "zone.js": "^0.8.16"
  },
  "devDependencies": {
    "@angular/cli": "1.2.7",
    "@angular/compiler-cli": "^4.0.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "^2.4.2"
  }
}
